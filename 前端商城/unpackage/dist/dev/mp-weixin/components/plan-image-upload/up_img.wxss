
.imageUploadContainer{
	padding: 10rpx 5rpx;
	margin: 10rpx 5rpx;
}
.name{
	text-align: center;
	margin-top:-30rpx;
}
.dragging{
	-webkit-transform: scale(1.2);
	        transform: scale(1.2)
}
.imageUploadList{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.imageItem, .imageUpload{
	width: 200rpx;
	height: 200rpx;
	margin: 30rpx 15rpx 30rpx;
}
.imageDel{
	position: relative;
	left: 180rpx;
	bottom: 225rpx;
	background-color: rgba(0,0,0,0.5);
	width: 36rpx;
	text-align: center;
	line-height: 35rpx;
	border-radius: 17rpx;
	color: white;
	font-size: 30rpx;
	padding-bottom: 2rpx;
}
.imageItem image, .moveImage{
	width: 200rpx;
	height: 200rpx;
	border-radius: 8rpx;
}
.imageUpload{
	line-height: 200rpx;
	text-align: center;
	font-size: 150rpx;
	color: #D9D9D9;
	border: 1px solid #D9D9D9;
	border-radius: 8rpx;
}
.moveImage{
	position: absolute;
}
.uni-list {
	width: 260px;
}
.popup_foot {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	border-top: 1px #F9F9F9 solid;
	line-height: 40px;
	height: 40px;
	margin-top: 30px;
}
.build_01{
	line-height: 35px;
}
.build_01 input{
	background: #ededed;
}
.popup_foot_l {
	width: 50%;
	text-align: center;
}
.popup_foot_r {
	width: 50%;
	text-align: center;
	border-left: 1px #F9F9F9 solid;
	color: #5095D3;
}

