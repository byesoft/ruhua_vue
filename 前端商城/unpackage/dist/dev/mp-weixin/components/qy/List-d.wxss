#list-d {
  width: 30%;
  padding: 15px 1.5%;
  display: inline-block;
}
#list-d .con_01 {
  background-color: #fff;
}
#list-d .con_01 ._img {
  width: 100%;
  height: 90px;
  margin-bottom: 5px;
}
#list-d .con_01_time {
  color: #FC455E;
  font-size: 10px;
  border: 1px solid #FC455E;
  display: inline;
  padding: 1px 5px;
}
#list-d .con_01_tit {
  height: 20px;
  overflow: hidden;
  line-height: 20px;
  font-size: 14px;
  padding-top: 3px;
}
#list-d .con_01_de {
  max-height: 36px;
  line-height: 18px;
  overflow: hidden;
  font-size: 10px;
}
#list-d .con_01_pri {
  color: #FC455E;
}
#list-d .blue .con_01_time {
  color: #2A82E4;
  border: 1px solid #2A82E4;
}
#list-d .blue .con_01_pri {
  color: #2A82E4;
}
#list-d .green .con_01_time {
  color: #34B07A;
  border: 1px solid #34B07A;
}
#list-d .green .con_01_pri {
  color: #34B07A;
}
#list-d .pink .con_01_time {
  color: #FF729A;
  border: 1px solid #FF729A;
}
#list-d .pink .con_01_pri {
  color: #FF729A;
}
#list-d .orange .con_01_time {
  color: #FF8D1B;
  border: 1px solid #FF8D1B;
}
#list-d .orange .con_01_pri {
  color: #FF8D1B;
}
#list-d .picss {
  position: relative;
}
#list-d .maiguang {
  position: absolute;
  top: 50%;
  left: 50%;
  z-index: 199;
  width: 100px;
  height: 100px;
  margin: -50px 0 0 -50px;
}
#list-d .maiguang ._img {
  width: 100px;
  height: 100px;
}
#list-d .containerss {
  background-color: #000;
  opacity: 0.3;
  width: 100%;
  height: 100%;
  z-index: 99;
  position: absolute;
  top: 0;
  left: 0;
}

