.coupon {
  background-color: #F8F8F8;
  min-height: 100vh;
  padding-bottom: 1px;
}
.coupon .po {
    z-index: 99;
    width: 100%;
}
.coupon .tab {
    padding: 10px 10px 0;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    width: 100%;
    font-size: 14px;
    background-color: #fff;
}
.coupon .bb {
    padding-bottom: 5px;
    text-align: center;
    width: 50%;
}
.coupon .xz {
    border-bottom: 2px solid red;
    padding-bottom: 5px;
    width: 50%;
    text-align: center;
}
.coupon .coupon {
    margin: 15px;
    background-color: #fff;
    border: 1px solid #EEEEEE;
    border-radius: 5px;
    box-shadow: 2px 2px 2px #EEEEEE;
    color: #9FA0A2;
    min-height: 10px;
    position: relative;
}
.coupon .coupon .ysy {
      position: absolute;
      right: 10px;
      top: 0;
}
.coupon .coupon .ysy ._img {
        width: 80px;
        height: 80px;
        z-index: 99;
}
.coupon .cou_t {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    padding: 20px 10px 10px;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
    font-size: 12px;
    width: 100%;
    box-sizing: border-box;
}
.coupon .cou_t_l {
    width: 20%;
    -webkit-flex-shrink: 0;
            flex-shrink: 0;
}
.coupon .cou_t_r {
    width: 55%;
    box-sizing: border-box;
    padding-left: 10px;
    -webkit-box-flex: 1;
    -webkit-flex-grow: 1;
            flex-grow: 1;
}
.coupon .cou_t_rr {
    width: 70px;
    background-color: #EB113E;
    color: #fff;
    height: 25px;
    line-height: 25px;
    font-size: 12px;
    text-align: center;
    margin: 15px 0 0 10px;
    border-radius: 20px;
}
.coupon .btn_grey {
    width: 70px;
    background-color: #9FA0A2;
    color: #fff;
    height: 25px;
    line-height: 25px;
    font-size: 12px;
    text-align: center;
    margin: 15px 0 0 10px;
    border-radius: 20px;
}
.coupon .cou_t_l_01 {
    color: #FF4444;
    font-size: 26px;
    padding-top: 8px;
}
.coupon .cou_t_l_01 ._span {
    font-size: 12px;
}
.coupon .cou_sss {
    font-size: 26px;
    padding-top: 8px;
}
.coupon .cou_sss ._span {
    font-size: 12px;
}
.coupon .cou_t_r_01 {
    font-size: 18px;
    color: #323233;
    padding: 3px 0 5px;
}
.coupon .cou_d {
    background-color: #FAFAFA;
    padding: 6px 15px;
    border-top: 1px dashed #EBEDF0;
    font-size: 12px;
}
.coupon .H50 {
    height: 50px;
}
.coupon .btn {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 45px;
    line-height: 45px;
    text-align: center;
    border-top: 1px solid #EEEEEE;
    z-index: 999;
    font-size: 14px;
    background-color: #fff;
}

