.kehu {
  font-size: 14px;
}
.kehu .search {
    background: #F4F4F4;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    width: 100%;
    box-sizing: border-box;
    padding: 10px;
}
.kehu .search .sear_01 {
      padding: 0 10px;
      height: 30px;
      border-radius: 5px;
      line-height: 30px;
      background-color: #fff;
      width: 100%;
}
.kehu .search .sear_01 ._span {
        padding-left: 10px;
        color: #ADADAD;
}
.kehu .search .sear_02 {
      width: 80px;
      padding: 3px 0 0 20px;
}
.kehu .tab {
    padding: 10px 10%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    width: 100%;
}
.kehu .tab .bb {
      padding-bottom: 5px;
      min-width: 80px;
      text-align: center;
}
.kehu .tab .xz {
      border-bottom: 2px solid red;
      padding-bottom: 5px;
      min-width: 80px;
      text-align: center;
}
.kehu .list {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    padding: 10px;
    border-bottom: 1px solid #EAEAEA;
}
.kehu .list .list_l {
      padding: 0 10px 0 0;
}
.kehu .list .list_l ._img {
        width: 50px;
        height: 50px;
        border-radius: 5px;
}
.kehu .list .list_r {
      line-height: 25px;
}
.kehu .list .list_r .list_r_01 .hui {
        border: 1px solid #FF6D6D;
        border-radius: 3px;
        color: #ff6d6d;
        font-size: 12px;
        padding: 0 5px;
        margin-left: 8px;
}
.kehu .list .list_r .list_r_02 {
        color: #ABABAB;
}
.kehu .p_btn {
    background: #F7F6FB;
    padding: 0 10px 0px;
    position: fixed;
    bottom: 0;
    width: 100%;
    z-index: 9999;
}
.kehu .pro_btn {
    background-color: #E5E5E5;
    padding: 10px;
    text-align: center;
    border-radius: 20px;
    background-color: #DF421D;
    color: #fff;
    width: 94%;
}

